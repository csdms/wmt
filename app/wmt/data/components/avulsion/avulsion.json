{
  "id":"avulsion",
  "name":"Avulsion",
  "url":"http://csdms.colorado.edu/wiki/Model_help:Avulsion",
  "provides":[
    {
      "id":"river"
    }
  ],
  "uses": [
    {
      "id":"discharge",
      "required":false
    },
    {
      "id":"elevation",
      "required":false
    }
  ],
  "parameters":[
    {
      "key":"simulation_name",
      "name":"Simulation name",
      "description":"Name of the simulation",
      "value":{
        "type":"string",
        "default":"Avulsion"
      }
    },
    {
      "key":"number_of_rows",
      "name":"Number of rows",
      "description":"Number of rows in the computational grid",
      "value":{
        "type":"int",
        "default":500,
        "range":{
          "min":0,
          "max":2147483647
        }
      }
    },
    {
      "key":"number_of_columns",
      "name":"Number of columns",
      "description":"Number of columns in the computational grid",
      "value":{
        "type":"int",
        "default":500,
        "range":{
          "min":0,
          "max":2147483647
        }
      }
    },
    {
      "key":"row_spacing",
      "name":"Row spacing",
      "description":"Grid resolution in cross-shore (row) direction",
      "value":{
        "type":"float",
        "default":100.0,
        "range":{
          "min":0.0,
          "max":10000.0
        },
        "units":"m"
      }
    },
    {
      "key":"column_spacing",
      "name":"Column spacing",
      "description":"Grid resolution in along-shore (column) direction",
      "value":{
        "type":"float",
        "default":100.0,
        "range":{
          "min":0.12,
          "max":10000.0
        },
        "units":"m"
      }
    },
    {
      "key":"output_format",
      "name":"Output format",
      "description":"File format for output files",
      "value":{
        "type":"choice",
        "default":"netcdf",
        "choices":[
          "netcdf",
          "vtk"
        ]
      }
    }
  ]
}
